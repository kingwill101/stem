# HMAC signing with TLS-enabled Redis
STEM_BROKER_URL=rediss://redis:6379/0
STEM_RESULT_BACKEND_URL=rediss://redis:6379/1
PORT=8081

# Replace with your own secret (32 bytes base64 encoded)
STEM_SIGNING_KEYS=primary:c3VwZXItc2VjcmV0LXN1cGVyLXNlY3JldA==
STEM_SIGNING_ACTIVE_KEY=primary

# TLS settings (certificates mounted via docker-compose)
STEM_TLS_CA_CERT=/certs/server.crt
STEM_TLS_ALLOW_INSECURE=true
